<MyLayout>
	search_box:search_box
	result:result

	BoxLayout:
		orientation:"vertical"
		size: root.width, root.height
		spacing:10
		padding:10

		GridLayout:
			cols:2
			size_hint_y:None
			height:50
			spacing:10

			TextInput:
				id:search_box
				multiline:False
			Button:
				text:"Search"
				on_press: root.search()

		TextInput:
			id:result
			multiline:True



